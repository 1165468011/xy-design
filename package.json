{
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "install2": "cd example/v2 && pnpm install",
    "switch2": "node bin/vue-switch.js 2 && vue-demi-switch 2",
    "switch3": "node bin/vue-switch.js 3 && vue-demi-switch 3",
    "dev": "pnpm switch3 && vite --config vite.demo.js",
    "build": "pnpm build3 && pnpm build2",
    "build3": "npx vue-demi-switch 3 && cd packages/xy-design vite build",
    "build2": "npx vue-demi-switch 2 && cd packages/xy-design-v2 && vite build",
    "precommit": "lint-staged",
    "lint-fix": "eslint --fix  --ext .vue,.js,.ts",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "format": "prettier-eslint --write src/**/*.{js,vue} && prettier-eslint --write",
    "pretty": "pretty-quick --staged",
    "lint": "eslint --ext .js,.vue,.ts,.json --ignore-path .gitignore . --fix",
    "prepare": "husky install"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint --edit $1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,vue,scss}": "prettier --write",
    "*.{ts,tsx,js,vue}": "eslint --fix"
  },
  "dependencies": {
    "@xy-design/components": "workspace:^0.0.1",
    "@xy-design/utils": "workspace:^0.0.1",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.6",
    "lodash": "^4.17.21",
    "vue-demi": "^0.13.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@types/lodash": "^4.14.188",
    "@typescript-eslint/parser": "^5.42.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "eslint": "^8.27.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.7.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.56.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vue-tsc": "^1.0.9"
  }
}
